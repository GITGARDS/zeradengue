  <body class="body-home">
    <header class="header">
      <div class="header-user">
        <div class="header-user-img">
          <i class="fa-solid fa-circle-user"></i>
        </div>
        <div class="header-user-name">
          <h2>usuario</h2>
        </div>
        <div class="header-sair">
          <i class="fa-solid fa-power-off"></i>
        </div>
      </div>
    </header>

    <main class="main-denuncia">
      <section class="sessao-denuncia">
        <form action="">

          <div class="imagem">
            <div class="icone-camera">
              <i class="fa-solid fa-camera"></i>
            </div>

            <div class="imagem-importada">
              <img src="" alt="imagem"/>
            </div>

          </div>

          <div class="form-group">
            <label for="endereco" class="sr-only">endereco</label>
            <input
              type="text"
              name="endereco"
              id="endereco"
              placeholder="endereco"
            />
          </div>

          <div class="form-group">
            <label for="cidade" class="sr-only">cidade</label>
            <input type="text" name="cidade" id="cidade" placeholder="cidade" />
          </div>

            <input class="btn btn-primary"  type="submit" value="Salvar" />

            <a class="btn-voltar" href="">
              <i class="fa-solid fa-chevron-left"></i>
              <span class="texto">Voltar</span>
            </a>

        </form>
      </section>
    </main>